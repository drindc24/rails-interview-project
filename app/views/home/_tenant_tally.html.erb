<div class="container mt-5">
  <h2>Tenant Hits</h2>
  <table class="table table-striped table-bordered">
    <thead>
    <tr>
      <th>Tenant ID</th>
      <th>Name</th>
      <th>Request Count</th>
    </tr>
    </thead>
    <tbody>
    <%- @tenants.each do |tenant| %>
      <tr>
        <td><%= tenant.id %></td>
        <td><%= tenant.name %></td>
        <td><%= tenant.request_count %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>